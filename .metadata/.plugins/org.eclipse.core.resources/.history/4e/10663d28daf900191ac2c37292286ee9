
public class Simbolo {
	public final static int IF    =0;
	public final static int ID    =1;	//
	public final static int GT    =2;	// >
	public final static int LT    =3;	// <
	public final static int GE    =4;	// >=
	public final static int LE    =5;	// <=
	public final static int BEQ   =6;	// =
	public final static int NE 	  =7;   // !=
	public final static int DELIM =8;   // ;  
	public final static int DIG   =9;   
	public final static int E     =10;
	public final static int CHAR  =11;  
	
	
	
	
	
	
	
		
	

}
